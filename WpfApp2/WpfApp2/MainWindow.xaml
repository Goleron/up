<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="ArtifactDgr" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding ArtifactName }"/>
                <DataGridTextColumn Binding="{Binding ArtifactDescription }"/>
                <DataGridTextColumn Binding="{Binding Century }"/>
                <DataGridTextColumn Binding="{Binding Museums.Name }"/>
                <DataGridTextColumn Binding="{Binding Museums.Address }"/>
                <DataGridTextColumn Binding="{Binding Museums.City }"/>
                <DataGridTextColumn Binding="{Binding Categories.CategoryName }"/>
                <DataGridTextColumn Binding="{Binding Categories.categoryDescription }"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
